        tools:layout="@layout/fragment_plant_agave"/>
    <fragment
        android:id="@+id/plantCarrotFragment"
        android:name="MX.TeosentliTeam.teosentli.ui.plants.PlantCarrotFragment"
        android:label="PlantCarrotFragment"
        tools:layout="@layout/fragment_plant_carrot"/>
    <fragment
        android:id="@+id/plantPotatoeFragment"
        android:name="MX.TeosentliTeam.teosentli.ui.plants.PlantPotatoeFragment"
