<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.messages.MessagesFragment">

    <SearchView
        android:id="@+id/barraBusqueda"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_width="409dp"
        android:layout_height="681dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="49dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/contacto1"
        android:layout_width="0dp"
        android:layout_height="200px"
        android:tooltipText="Contacto 1"
        android:src="@drawable/ic_menu_messagges"
        app:layout_constraintEnd_toStartOf="@+id/contacto2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barraBusqueda"
        tools:src="@drawable/ic_menu_messagges"
        />

    <ImageButton
        android:id="@+id/contacto2"
        android:layout_width="0dp"
        android:layout_height="200px"
        android:tooltipText="Contacto2"
        android:src="@drawable/ic_menu_messagges"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/contacto1"
        app:layout_constraintTop_toBottomOf="@+id/barraBusqueda"
        tools:src="@drawable/ic_menu_messagges" />



</androidx.constraintlayout.widget.ConstraintLayout>